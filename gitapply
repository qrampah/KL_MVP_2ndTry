diff --git a/Kargoline/src/pages/JobDetailsPage.tsx b/Kargoline/src/pages/JobDetailsPage.tsx
new file mode 100644
index 0000000000000000000000000000000000000000..c143c42fcb0248b36e1fe3f2bc3e65ff372c2043
--- /dev/null
+++ b/Kargoline/src/pages/JobDetailsPage.tsx
@@ -0,0 +1,24 @@
+import React from 'react';
+import { View, Text, StyleSheet } from 'react-native';
+import { RouteProp } from '@react-navigation/native';
+
+interface Props {
+  route: RouteProp<any>;
+}
+
+const JobDetailsPage: React.FC<Props> = ({ route }) => {
+  const { id } = route.params || {};
+  return (
+    <View style={styles.container}>
+      <Text style={styles.title}>Job Details</Text>
+      {id && <Text>Job ID: {id}</Text>}
+    </View>
+  );
+};
+
+const styles = StyleSheet.create({
+  container: { flex: 1, alignItems: 'center', justifyContent: 'center' },
+  title: { fontSize: 24, marginBottom: 8 },
+});
+
+export default JobDetailsPage;
